<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape>
            <stroke android:color="#ffffff" android:width="1dp"></stroke>
            <corners android:radius="30dp"></corners>

